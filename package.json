{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"convex dev\"",
    "dev:next": "next dev",
    "dev:convex": "convex dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check app components contexts convex hooks lib",
    "lint:fix": "biome check --write --unsafe app components contexts convex hooks lib",
    "lint:md": "markdownlint-cli2 '*.md' 'docs/*.md' 'convex/*.md' '.claude/*.md'",
    "lint:md:fix": "markdownlint-cli2 --fix '*.md' 'docs/*.md' 'convex/*.md' '.claude/*.md'",
    "typecheck": "tsc --noEmit",
    "check": "biome check app components contexts convex hooks lib && tsc --noEmit",
    "fix": "biome check --write --unsafe app components contexts convex hooks lib",
    "test": "vitest",
    "test:once": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/backend": "^2.27.0",
    "@clerk/nextjs": "^6.36.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-google-maps/api": "^2.20.7",
    "@sentry/nextjs": "^10.29.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vladmandic/face-api": "^1.7.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.559.0",
    "next": "16.0.8",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "resend": "^6.6.0",
    "sonner": "^2.0.7",
    "svix": "^1.82.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@edge-runtime/vm": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "convex-test": "^0.0.41",
    "husky": "^9.1.7",
    "lefthook": "^2.0.9",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "ultracite": "6.3.10",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css}": [
      "biome format --write"
    ]
  }
}
